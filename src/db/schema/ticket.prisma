model Ticket {
    idTicket String @id @default(cuid())
    idUser String
    idEvent String
    amount Int
    participants Int
    createdAt DateTime @default(now())
    user User @relation(fields: [idUser], references: [idUser])
    event Event @relation(fields: [idEvent], references: [idEvent], onDelete: Cascade)
    detail TicketDetail[]
}   